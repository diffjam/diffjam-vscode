{
  "name": "diffjam-vscode",
  "displayName": "diffjam",
  "description": "custom linter and ratcheted build tool",
  "version": "0.1.2",
  "publisher": "DiffJam",
  "private": true,
  "license": "MIT",
  "homepage": "https://diffjam.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/diffjam/diffjam-vscode"
  },
  "engines": {
    "vscode": "^1.26.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "title": "Enable diffjam",
        "command": "diffjam.enable",
        "category": "diffjam"
      },
      {
        "title": "Disable diffjam",
        "command": "diffjam.disable",
        "category": "diffjam"
      }
    ],
    "configuration": {
      "properties": {
        "diffjam.enable": {
          "type": "boolean",
          "default": true
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint . --ext .ts,.tsx",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/node": "^12.12.0",
    "@types/vscode": "^1.26.0",
    "@typescript-eslint/eslint-plugin": "^4.16.0",
    "@typescript-eslint/parser": "^4.16.0",
    "eslint": "^7.21.0",
    "typescript": "^4.3.5",
    "vsce": "^2.3.0"
  },
  "dependencies": {
    "@types/lodash": "^4.14.176",
    "diffjam": "3.1.0",
    "lodash": "^4.17.21",
    "vscode-languageclient": "^7.0.0"
  }
}